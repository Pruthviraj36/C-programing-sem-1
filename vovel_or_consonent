#include <stdio.h>

int main()
{
	char x;
	printf("\t\t\t\tVovel or Consonent\n\n\n");
	printf("\tEnter an Alphabet : ");
	scanf("%c", &x);
	if ((x == 'a') || (x == 'e') || (x == 'i') || (x == 'o') || (x == 'O') || (x == 'u') || (x == 'A') || (x == 'E') || (x == 'I') || (x == 'U'))
	{
		printf("\n\tThe alphabet is vovel ");
	}
	else
	{
		printf("\n\tThe alphabet is consonent");
	}
}
